# Local PostgreSQL Database Configuration
# Copy this file to .env and update with your database details

# Option 1: Use individual connection parameters (recommended for local setup)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pipeline_system
DB_USER=postgres
DB_PASSWORD=your_password_here

# Option 2: Or use a complete DATABASE_URL (leave empty if using individual parameters above)
# DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Application Settings
DEBUG=true
LOG_LEVEL=INFO